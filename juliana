soma = sum([1, 2, 3])

media = sum([1, 2, 3])

frase = "Juliana é poderosa."

nova_frase = frase.replace("poderosa", "incrível")

var n = 5;
var aux = "";
for (var linha = 1; linha <= n; linha++) {
  for (var coluna = 1; coluna <= n; coluna++) {
    aux += "*";
  }
  console.log(aux);
  aux = "";
}
